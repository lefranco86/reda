<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Employee.
    -->
    <changeSet id="20180925145725-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="contact_information_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_contact_information_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact_information"/>

        <addForeignKeyConstraint baseColumnNames="entreprise_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_entreprise_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entreprise"/>

    </changeSet>
</databaseChangeLog>
